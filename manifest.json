{
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "css": [
        "assets/css/style.css"
      ],
      "js": [
        "assets/lib/jquery-3.7.1.min.js",
        "js/data.js",
        "js/content-script.js",
        "assets/lib/tailwindcss-3.4.3.js",
        "injects/tailwind.config.js"
      ],
      "matches": [
        "http://localhost/*"
      ],
      "run_at": "document_idle"
    }
  ],
  "description": "...",
  "homepage_url": "https://www.google.com",
  "icons": {
    "128": "assets/img/check.png",
    "48": "assets/img/check.png",
    "16": "assets/img/check.png"
  },
  "manifest_version": 3,
  "name": "SEO checker",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0.0",
  "web_accessible_resources": [
    {
      "resources": [
        "assets/img/logo.png",
        "assets/img/bg.jpg",
        "assets/img/check.png",
        "assets/img/wait-for-reaction.gif",
        "injects/content-template.html",
        "assets/lib/tailwindcss-3.4.3.js",
        "injects/tailwind.config.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}
